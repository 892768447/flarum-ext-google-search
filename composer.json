    "keywords": [
        "extension",
        "flarum",
        "google",
        "search"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [{
        "name": "Irony",
        "email": "892768447@qq.com",
        "homepage": "https://pyqt.site/"
